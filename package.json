{
  "name": "microstates",
  "version": "0.6.1",
  "description": "Graceful state transitions for data of any shape.",
  "keywords": [
    "blood",
    "immutable",
    "microstates"
  ],
  "homepage": "https://github.com/cowboyd/microstates.js#readme",
  "bugs": {
    "url": "https://github.com/cowboyd/microstates.js/issues"
  },
  "license": "MIT",
  "author": "Charles Lowell <charles@frontside.io>, Taras Mankovski <taras@thisdot.co>",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "main": "dist/microstates.umd.js",
  "module": "dist/microstates.js",
  "browser": "dist/microstates.umd.js",
  "repository": "git+ssh://git@github.com/cowboyd/microstates.js.git",
  "scripts": {
    "build": "rollup --config",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "prerelease": "npm test",
    "release": "npm pack",
    "prepublish": "npm test",
    "test": "jest",
    "start": "npm test -- --watch"
  },
  "dependencies": {
    "funcadelic": "0.3.3",
    "object.getownpropertydescriptors": "2.0.3",
    "ramda": "0.25.0",
    "symbol-observable": "1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "babel-cli": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.6.0",
    "husky": "0.14.3",
    "jest": "^22.1.0",
    "jsonfile": "^4.0.0",
    "lint-staged": "6.0.0",
    "prettier": "^1.10.2",
    "pretty-format": "22.1.0",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^4.0.0-beta.0",
    "rollup-plugin-filesize": "1.5.0",
    "rxjs": "6.0.0-alpha.3",
    "standard-version": "^4.2.0"
  },
  "jest": {
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(js)$",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globalSetup": "./build.js",
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "collectCoverageFrom": [
      "dist/microstates.umd.js"
    ]
  }
}
